@charset "UTF-8";

.orders{ width: 100%; overflow: hidden;}
.orders .tabsCont{ display: none; width: 100%; }




.tabsCont2{ }
/* 发货信息 */
.tabsCont2 ul{ margin-bottom: 18px;}
.tabsCont2 ul li{ border: 1px solid #ccc;  width: 600px; background: #fff; border-radius:16px; margin:0px auto; height:310px; margin-top:20px;}
.tabsCont2 ul li .tit{ background: #fff; border-bottom: 1px solid #ccc; }
.tabsCont2 ul li .tit div{ height: 60px; line-height: 60px; padding: 0px 25px; clear: both; }
.tabsCont2 ul li .tit div.code{ font-size: 28px;}
.tabsCont2 ul li .tit div.code i.icon_m{ float: right; font-size: 24px; color: #2b98e5; display: inline-block; margin-left: 16px;}
.tabsCont2 ul li .tit div.code i img{ vertical-align: middle; margin-right: 8px;}
.tabsCont2 ul li .tit div.dtime{ font-size: 24px; }

.tabsCont2 ul li .con{background: #fff; background: #fff; padding: 15px 25px; overflow: hidden; clear: both; }
.tabsCont2 ul li .con .conlist{ float: left; font-size: 24px; clear: both; overflow: hidden; padding: 10px 0px;font-weight: lighter;}
.tabsCont2 ul li .con .conlist label{ height: 50px; float: left;font-size: 24px;  font-weight: lighter;}
.tabsCont2 ul li .foot{  height: 80px; line-height: 80px; border-top: 1px solid #ccc; margin: 0 25px; overflow: hidden;}
.tabsCont2 ul li .foot span{ font-size: 24px; float: left; }
.tabsCont2 ul li .foot span:nth-child(1){ text-align: left;}
.tabsCont2 ul li .foot span:nth-child(2){ text-align: center;}
.tabsCont2 ul li .foot span:nth-child(3){ text-align: right;}



/* 付款信息 */
.tabsCont3 dl{ background: #fff; overflow: hidden; padding:30px 0px; border: 1px solid #ccc; margin-bottom: 20px; width:610px; border-radius:6px; margin:20px auto 0;}
.tabsCont3 dl:first-child{ border-top: 0px;}
.tabsCont3 dl dt{ width: 70px; float: left; margin-top: 10px; margin-left: 50px;}
.tabsCont3 dl dt span{ display: block; white-space:nowrap; color: #666; }
.tabsCont3 dl dt span:nth-child(1){ font-size: 24px; text-align: center;  }
.tabsCont3 dl dt span:nth-child(2){ font-size: 18px; color: #666; text-align: center; }
.tabsCont3 dl dd{ float: left;  overflow: hidden;}
.tabsCont3 dl dd .one{ width: 160px; height: 53px; float: left; text-align: center;}
.tabsCont3 dl dd .one img{ margin-bottom: 10px;}
.tabsCont3 dl dd .one p{ font-size: 20px; color: #666;}
.tabsCont3 dl dd .two{ width: 350px; float: left;}
.tabsCont3 dl dd .two p:nth-child(1){ font-size: 28px; padding: 5px 0px;}
.tabsCont3 dl dd .two p:nth-child(2){ font-size: 24px; padding: 5px 0px;font-weight: lighter;}

.p24{font-size:28px; line-height:60px; margin-right:215px;} 
.round {
	background:#fff;
    border: 1px solid #fff;
	width:94%;
    -moz-border-radius: 15px;      /* Gecko browsers */
    -webkit-border-radius: 15px;   /* Webkit browsers */
    border-radius:15px;            /* W3C syntax */
	margin:20px 10px 0px 15px;
	padding-top:10px;
}
.tabsCont3 ul{ font-size:24px; margin-top:30px;}
.tabsCont3 ul li{  height:50px; line-height:50px; padding-left:10px;padding-right: 10px;}
.tabsCont3 ul li b{ float:right; font-weight:normal;}
.tabsCont3 p{ height:70px; line-height:70px; border-bottom:1px solid #ccc; padding:10px;}


.tabsCont1  ul{ width: 100%;}
.tabsCont1  ul li{ background: #fff; padding: 15px 15px 15px 15px; position: relative; margin-bottom: 20px; /*border-bottom: 1px solid #ccc;*/ overflow: hidden;}
.tabsCont1  ul li .productInfo{ width: 100%; border-bottom: 1px solid #ccc; padding-bottom: 45px;}
.tabsCont1  ul li .productCode{ line-height: 50px; margin-bottom: 10px;}
.tabsCont1  ul li .productCode,.productName{ font-size: 28px; padding-bottom:20px; clear: both;font-weight: lighter; }
.tabsCont1  ul li .productCode,.productName2{ font-size: 24px; padding: 30px 25px 0px 25px; clear: both;font-weight: lighter;}
.tabsCont1  ul li .productCode a.J_del{ padding-left: 36px; padding-right: 25px; position: absolute; right: 0px; background: url("../images/icon_del.png") no-repeat 0px 12px;}
.tabsCont1  ul li label{ height: 50px; display: block; float: left;}
.orderPrice{ height: 100px; line-height: 100px; background: #fff; border-top: 1px solid #ccc; border-bottom: 1px solid #ccc; overflow: hidden; padding:0px 15px;
position: fixed;bottom: 0px;width: 100%;}
.orderPrice span{padding: 0px 0px; font-size: 30px; }
.tabsCont1  ul li .productPrice{ padding: 25px 25px; height: 27px; line-height: 27px; clear: both;}
.spacial{font-size: 24px;font-weight: lighter;}
.tabsCont1  ul li .productPrice .words{
	font-size: 24px;
	font-weight: normal;
}
.tabsCont1  ul li .productPrice .spinner{ width: 300px; float: right; position: relative; font-size: 24px; }
.tabsCont1  ul li .productPrice .spinner a{ width: 44px; height: 52px; line-height: 52px; display: inline-block; background: #fff; font-size: 30px;border-bottom: 1px solid #ccc;  text-align: center; position: absolute; top: 0px;}
.tabsCont1  ul li .productPrice .min{ left: 100px; }
.tabsCont1  ul li .productPrice .add{ left: 254px; }
.tabsCont1  ul li .productPrice em.number{ width: 110px; height: 52px; line-height: 52px; font-size: 28px;  border-bottom: 1px solid #ccc; left: 144px; text-align: center; position: absolute; top: 0px; }
.tabsCont1  ul li .productPrice em{ float: right; }
